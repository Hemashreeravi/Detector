import uuid

def generate_id():
    """
    Function to generate a unique ID using UUID.
    
    Returns:
    - str: A unique ID.
    """
    return str(uuid.uuid4())

def main():
    # Generate a unique ID
    new_id = generate_id()
    
    # Print the generated ID
    print(f"Generated ID: {new_id}")

if __name__ == "__main__":
    main()
